#!/usr/bin/env php
<?php

require __DIR__ . '/../vendor/autoload.php';

use Symfony\Component\Console\Application;
use Xterr\NaceCodes\Command\BuildTranslationsCommand;
use Xterr\NaceCodes\Command\ParseRdfCommand;

$application = new Application('NACE Codes CLI', '2.0.0');
$application->add(new BuildTranslationsCommand());
$application->add(new ParseRdfCommand());
$application->run();
